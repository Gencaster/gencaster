{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@antfu"
  ],
  "ignorePatterns": ["**/graphql/graphql.ts"],
  "rules": {
    "no-console": "off",
    "no-alert": "off",
    "vue/component-tags-order": ["error", {
      "order": [["template", "script"], "style"]
    }]
  }
}
