{
    "python.analysis.typeCheckingMode": "basic",
    "cSpell.words": [
        "Gencaster",
        "pythonosc",
        "ssml"
    ],
    "prettier.enable": false,
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ]
}
